<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="850.0" prefWidth="1000.0" style="-fx-background-color: #FFF;"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.Controllers.AdminDashboard">
    <children>
        <AnchorPane prefHeight="842.0" prefWidth="294.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="942.0" AnchorPane.topAnchor="0.0">
            <children>
            <VBox prefHeight="850.0" prefWidth="300.0" style="-fx-background-color: #EBE8F9;"
                  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
            <ImageView fitHeight="175.0" fitWidth="318.0" pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@../../images/admin.png"/>
                </image>
                <VBox.margin>
                    <Insets bottom="10.0" left="50.0"/>
                </VBox.margin>
            </ImageView>
            <Button fx:id="overviewBtn" alignment="BASELINE_LEFT" graphicTextGap="20.0" mnemonicParsing="false"
                    onAction="#handleButtonAction" prefHeight="52.0" prefWidth="317.0"
                    style="-fx-background-color: #EBE8F9;" text="Overview">
                <padding>
                    <Insets left="50.0"/>
                </padding>
                <graphic>
                    <ImageView fitHeight="26.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@../../images/home.png"/>
                        </image>
                    </ImageView>
                </graphic>
            </Button>
            <Button fx:id="staffBtn" alignment="BASELINE_LEFT" graphicTextGap="20.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="52.0" prefWidth="317.0" style="-fx-background-color: #EBE8F9;" text="Staff">
                <padding>
                    <Insets left="50.0" />
                </padding>
                <graphic>
                    <ImageView fitHeight="19.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@../../images/staff.png" />
                        </image>
                    </ImageView>
                </graphic>
            </Button>
            <Button fx:id="roomsBtn" alignment="BASELINE_LEFT" graphicTextGap="20.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="52.0" prefWidth="317.0" style="-fx-background-color: #EBE8F9;" text="Rooms">
                <padding>
                    <Insets left="50.0" />
                </padding>
                <graphic>
                    <ImageView fitHeight="26.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@../../images/rooms.png" />
                        </image>
                    </ImageView>
                </graphic>
            </Button>
            <Button fx:id="paymentsBtn" alignment="BASELINE_LEFT" graphicTextGap="20.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="52.0" prefWidth="317.0" style="-fx-background-color: #EBE8F9;" text="Payments">
                <padding>
                    <Insets left="50.0" />
                </padding>
                <graphic>
                    <ImageView fitHeight="26.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@../../images/bills.png" />
                        </image>
                    </ImageView>
                </graphic>
            </Button>


        </AnchorPane>
    </children>
</AnchorPane>